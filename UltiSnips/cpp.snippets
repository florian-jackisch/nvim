endsnippet

snippet rule_of_6 "Rule of 6"
${1:T}() = ${2:default};
$1(const $1&) = ${3:default};
$1& operator=(const $1&) = $3;
$1($1&&) = ${4:default};
$1& operator=($1&&) = $4;
~$1() = ${5:default};
endsnippet

snippet namespace "namespace"
namespace $1 {
$0
}  // namespace $1
endsnippet

snippet ignored "ignore diagnostic"
#pragma GCC diagnostic push
#pragma GCC diagnostic ignored "-W${1:uninitialized}"
#pragma GCC diagnostic pop$0
endsnippet
